last=0;
for dir in $(ls %deploy_to/releases/);
do
	if [ $dir -gt $last ]; then last=$dir; fi;
done;

pre_last=0;
for dir in $(ls %deploy_to/releases/);
do
	if [ $dir != $last ]; 
	then 
		if [ $dir -gt $pre_last ]; 
		then 
			pre_last=$dir;
		fi;
	fi;
done;

echo "last_release=%deploy_to/releases/$last; previous_last_release=%deploy_to/releases/$pre_last";